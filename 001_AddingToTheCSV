#This is meant to add onto the already existing csv file with a new player.
#Creating a second DataFrame
game_data2 = [    
    ["John", 0.9  , 9.0, "OnePiece"], 
    ["John" , 2.7, 2.4, "DBFighterZ"], 
    ["John", 2.1  , 8.7, "Halo"]
]
#Adding the new DataFrame to the previous one, creating the newest DataFrame.
game_data2 = pd.DataFrame(game_data2, index = ["9", "10", "11"], columns =["Name", "Hours_Played", "Score", "Game"])
pd.concat([game_data, game_data2])
